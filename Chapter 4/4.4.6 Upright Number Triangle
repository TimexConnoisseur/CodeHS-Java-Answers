public class NumberTriangle
{
    public static void main(String[] args)
    {
       // Call makeNumberTriangle 
       makeNumberTriangle();
    }
    
    // Makes an upright triangle with the numbers 1-5
    public static void makeNumberTriangle()
    {
      for (int i = 1; i <= 5; i++)
      {
          for (int k = 1; k <= i; k++)
          {
              System.out.print(k + " ");
          }
          System.out.println();
      }// Your code goes here! 
    }
}
